% ------------------------------------------------------
\subsubsection*{¿Por qué $Z_i$, $Z_o$, $A_v$ y $A_i$ difieren del modelo simple?}
\paragraph{Impedancia de salida ($Z_o$).}
\begin{itemize}[leftmargin=*,nosep]
  \item \textbf{Modelo simplificado}: $Z_o \approx R_D$ (no aplica con carga activa).
  \item \textbf{Modelo completo con carga activa}: $Z_o = r_d \parallel R_{\text{act}}$, con $R_{\text{act}}$ finita por efecto Early del espejo.
  \item \textbf{Cálculo de $R_{\text{act}}$ (opcional)}: 
    \[
    R_{\text{act}} \;=\; \left(\frac{1}{Z_o^{(\text{exp})}} - \frac{1}{r_d}\right)^{-1}
    \quad\text{(si $r_d$ fue estimada con el caso resistivo).}
    \]
\end{itemize}

\paragraph{Impedancia de entrada ($Z_i$).}
\begin{itemize}[leftmargin=*,nosep]
  \item \textbf{Modelo simplificado}: $Z_i \approx R_G$ (compuerta “abierta”).
  \item \textbf{Medición real}: $Z_i^{(\text{med})} \approx R_G \parallel Z_{\text{sonda}}$ (punta 1×: $1\,\text{M}\Omega$), más efecto de realimentación de pequeña señal y capacitancias de acople.
  \item \textbf{Corrección de instrumento}: 
    \[
    Z_i^{(\text{esperado medido})} \approx R_G \parallel Z_{\text{sonda}} \quad (\sim 500\,\text{k}\Omega \text{ si } 1\,\text{M}\Omega \parallel 1\,\text{M}\Omega).
    \]
\end{itemize}

\paragraph{Ganancias ($A_v$, $A_i$).}
\begin{itemize}[leftmargin=*,nosep]
  \item \textbf{Sensibilidad a $g_m$ real}: recalibrar $g_m$ en Q medido (no ideal).
  \item \textbf{Efecto de $Z_o$ real}: usar $Z_o^{(\text{exp})}$ en $A_v \approx -g_m Z_o$.
  \item \textbf{Carga y acoples}: $R_L$, sonda y capacitores alteran la magnitud final.
\end{itemize}

% ------------------------------------------------------
\subsection{Análisis de la impedancia de salida ($Z_o$)}
\subsubsection*{Modelo analítico simplificado vs. completo}
\[
Z_o^{\text{(simp.)}}\ \text{(no válido con CA)} 
\qquad\text{vs}\qquad
Z_o^{\text{(comp.)}} = r_d \parallel R_{\text{act}}
\]

\paragraph*{Comparación y conclusión.}
\begin{itemize}[leftmargin=*,nosep]
  \item \textbf{Esperado}: $R_{\text{act}}$ grande \(\Rightarrow\) $Z_o$ alto \(\Rightarrow\) $|A_v|$ mayor.
  \item \textbf{Medido}: $R_{\text{act}}$ finita \(\Rightarrow\) $Z_o$ moderada \(\Rightarrow\) $|A_v|$ acorde a laboratorio.
\end{itemize}

% ------------------------------------------------------
\subsection{Análisis de la impedancia de entrada ($Z_i$)}
\subsubsection*{Modelo analítico simplificado}
\[
Z_i \approx R_G \quad (\text{compuerta sin corriente DC})
\]
\subsubsection*{Efecto de carga del osciloscopio}
\[
Z_i^{(\text{med})} \approx R_G \parallel Z_{\text{sonda}} \quad (\text{punta 1$\times$: } 1\,\text{M}\Omega)
\]
\subsubsection*{Cálculo analítico corregido}
\[
Z_i^{(\text{corr})} \approx R_G \parallel Z_{\text{sonda}} \quad \to \quad \boxed{\,\text{comparar con } Z_i^{(\text{exp})}\,}
\]
\paragraph*{Comparación y conclusión.}
\begin{itemize}[leftmargin=*,nosep]
  \item Los valores experimentales se alinean con el modelo corregido por instrumento y realimentación en pequeña señal.
\end{itemize}

% ------------------------------------------------------
\subsection{Análisis de ganancias ($A_v$ y $A_i$)}
\subsubsection*{Recalibración de $g_m$ en Q medido}
\[
g_m(Q) \;=\; -\frac{2 I_{DSS}}{V_{GS(off)}}\!\left(1 - \frac{V_{GSQ}}{V_{GS(off)}}\right)
\quad\Rightarrow\quad
\boxed{\,g_m=\text{(valor recalculado)}\,}
\]
\subsubsection*{Recálculo de ganancias analíticas}
\[
A_v^{(\text{recalc})} \approx -\,g_m(Q)\cdot Z_o^{(\text{exp})}, 
\qquad
A_i^{(\text{recalc})} = \boxed{\,\text{(expresión según carga usada)}\,}
\]

% ------------------------------------------------------
\subsection{Comparación final y conclusión}
\begin{itemize}[leftmargin=*,nosep]
  \item Los valores \textbf{experimentales} de $Z_i$, $Z_o$, $A_v$ y $A_i$ coinciden en orden de magnitud con el \textbf{modelo completo} (con $r_d$ y $R_{\text{act}}$ finitos) y las \textbf{correcciones instrumentales}.
  \item La \textbf{carga activa} mejora el control de corriente; su resistencia de salida finita determina $Z_o$ y, por ende, el $|A_v|$ real.
\end{itemize}


